From d693b43546c72b0929029c03deb376440b6bf951 Mon Sep 17 00:00:00 2001
From: Bogdan Folea <bogdan.folea@nxp.com>
Date: Fri, 11 Mar 2022 11:42:17 +0200
Subject: [PATCH 01/11] dt-bindings: crypto: hse: fix MU node example

Issue: ALB-7963
Upstream-Status: Pending 

Signed-off-by: Bogdan Folea <bogdan.folea@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 .../devicetree/bindings/crypto/nxp-hse.yaml   | 32 +++++++------------
 1 file changed, 12 insertions(+), 20 deletions(-)

diff --git a/Documentation/devicetree/bindings/crypto/nxp-hse.yaml b/Documentation/devicetree/bindings/crypto/nxp-hse.yaml
index 5c7ff062dbc6..1e68ceb2f688 100644
--- a/Documentation/devicetree/bindings/crypto/nxp-hse.yaml
+++ b/Documentation/devicetree/bindings/crypto/nxp-hse.yaml
@@ -51,24 +51,16 @@ required:
 
 examples:
   - |
-    hse: crypto {
-        compatible = "simple-bus";
-        #address-cells = <2>;
-        #size-cells = <2>;
-        #interrupt-cells = <3>;
-        ranges;
-
-        mu0b@40210000 {
-                compatible = "fsl,s32gen1-hse";
-                reg = <0x0 0x40210000 0x0 0x1000>,
-                      <0x0 0x22C00000 0x0 0x1000>;
-                reg-names = "hse-mu0b-regs",
-                            "hse-mu0b-desc";
-                interrupts = <GIC_SPI 103 IRQ_TYPE_EDGE_RISING>,
-                             <GIC_SPI 104 IRQ_TYPE_EDGE_RISING>,
-                             <GIC_SPI 105 IRQ_TYPE_EDGE_RISING>;
-                interrupt-names = "hse-mu0b-ack",
-                                  "hse-mu0b-rx",
-                                  "hse-mu0b-err";
-        };
+    mu0b@40210000 {
+            compatible = "fsl,s32gen1-hse";
+            reg = <0x40210000 0x1000>,
+                  <0x22C00000 0x1000>;
+            reg-names = "hse-mu0b-regs",
+                        "hse-mu0b-desc";
+            interrupts = <0 103 1>,
+                         <0 104 1>,
+                         <0 105 1>;
+            interrupt-names = "hse-mu0b-ack",
+                              "hse-mu0b-rx",
+                              "hse-mu0b-err";
     };

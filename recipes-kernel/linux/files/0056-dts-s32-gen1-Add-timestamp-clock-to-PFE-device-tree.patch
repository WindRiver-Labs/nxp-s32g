From 258ee8f1ffb88f18e78df448e38bcb5f66f99a0f Mon Sep 17 00:00:00 2001
From: Ondrej Spacek <ondrej.spacek@nxp.com>
Date: Mon, 1 Feb 2021 12:34:53 +0100
Subject: [PATCH 56/78] dts: s32-gen1: Add timestamp clock to PFE device tree.

Issue: ALB-6407
Signed-off-by: Ondrej Spacek <ondrej.spacek@nxp.com>
---
 arch/arm64/boot/dts/freescale/fsl-s32g274a.dtsi | 5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-s32g274a.dtsi b/arch/arm64/boot/dts/freescale/fsl-s32g274a.dtsi
index 6594e6a3b919..d100fd7219e4 100644
--- a/arch/arm64/boot/dts/freescale/fsl-s32g274a.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-s32g274a.dtsi
@@ -68,8 +68,9 @@
 		interrupt-names = "hif0", "hif1", "hif2", "hif3",
 				  "bmu", "nocpy", "upegpt", "safety";
 		clocks = <&clks S32G274A_SCMI_CLK_PFE_AXI>,
-			 <&clks S32G274A_SCMI_CLK_PFE_PE>;
-		clock-names = "pfe_sys", "pfe_pe";
+			 <&clks S32G274A_SCMI_CLK_PFE_PE>,
+			 <&clks S32G274A_SCMI_CLK_PFE_TS>;
+		clock-names = "pfe_sys", "pfe_pe", "pfe_ts";
 		firmware-name = "s32g_pfe_class.fw";
 		fsl,fw-class-name = "s32g_pfe_class.fw";
 		fsl,fw-util-name = "s32g_pfe_util.fw";

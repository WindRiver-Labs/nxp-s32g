From ded18e5088e4cfd4fe744325751cc404a9ddef4e Mon Sep 17 00:00:00 2001
From: Radu Pirea <radu-nicolae.pirea@nxp.com>
Date: Wed, 16 Dec 2020 09:47:18 +0200
Subject: [PATCH 33/76] s32-gen1: add gpio nodes

Add gpio nodes for s32g274a and s32r45.

Issue: ALB-5669

Signed-off-by: Radu Pirea <radu-nicolae.pirea@nxp.com>
---
 arch/arm/dts/fsl-s32g274a.dtsi | 17 +++++++++++++++++
 arch/arm/dts/fsl-s32r45.dts    | 16 ++++++++++++++++
 2 files changed, 33 insertions(+)

diff --git a/arch/arm/dts/fsl-s32g274a.dtsi b/arch/arm/dts/fsl-s32g274a.dtsi
index 9583b9a032..c6864a6d87 100644
--- a/arch/arm/dts/fsl-s32g274a.dtsi
+++ b/arch/arm/dts/fsl-s32g274a.dtsi
@@ -173,6 +173,14 @@
 			<&pinctrl0 512 595>;
 			status = "okay";
 		};
+
+		gpio0: siul2-gpio0 {
+			compatible = "fsl,s32-gen1-siul2-gpio";
+			#gpio-cells = <2>;
+			gpio-controller;
+			gpio-ranges = <&pinctrl0 0 0 102>;
+			status = "okay";
+		};
 	};
 
 	siul2_1@44010000 {
@@ -189,6 +197,15 @@
 			<&pinctrl1 631 1023>;
 			status = "okay";
 		};
+
+		gpio1: siul2-gpio1 {
+			compatible = "fsl,s32-gen1-siul2-gpio";
+			#gpio-cells = <2>;
+			gpio-controller;
+			gpio-ranges = <&pinctrl1 0 112 11>,
+				<&pinctrl1 11 144 47>;
+			status = "okay";
+		};
 	};
 };
 
diff --git a/arch/arm/dts/fsl-s32r45.dts b/arch/arm/dts/fsl-s32r45.dts
index cf4595f4ef..5362abf8a5 100644
--- a/arch/arm/dts/fsl-s32r45.dts
+++ b/arch/arm/dts/fsl-s32r45.dts
@@ -71,6 +71,14 @@
 			<&pinctrl0 512 573>;
 			status = "okay";
 		};
+
+		gpio0: siul2-gpio0 {
+			compatible = "fsl,s32gen1-siul2-gpio";
+			#gpio-cells = <2>;
+			gpio-controller;
+			gpio-ranges = <&pinctrl0 0 0 102>;
+			status = "okay";
+		};
 	};
 
 	siul2_1@4403C000 {
@@ -87,6 +95,14 @@
 			<&pinctrl1 603 785>;
 			status = "okay";
 		};
+
+		gpio1: siul2-gpio1 {
+			compatible = "fsl,s32gen1-siul2-gpio";
+			#gpio-cells = <2>;
+			gpio-controller;
+			gpio-ranges = <&pinctrl1 0 102 32>;
+			status = "okay";
+		};
 	};
 };
 

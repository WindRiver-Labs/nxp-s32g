 #if !CONFIG_IS_ENABLED(FSL_PFENG_EMAC_0_RGMII)
@@ -444,7 +444,7 @@ static void setup_iomux_pfe(struct udevice *dev,
 		break;
 
 	case PHY_INTERFACE_MODE_RGMII:
-#if CONFIG_IS_ENABLED(TARGET_S32G274AEVB) || \
+#if CONFIG_IS_ENABLED(TARGET_S32G2XXAEVB) || \
 		CONFIG_IS_ENABLED(TARGET_S32G274ARDB) || \
 		CONFIG_IS_ENABLED(TARGET_S32G3XXAEVB)
 #if CONFIG_IS_ENABLED(FSL_PFENG_EMAC_0_RGMII)
@@ -473,7 +473,7 @@ static void setup_iomux_pfe(struct udevice *dev,
 		break;
 
 	case PHY_INTERFACE_MODE_RGMII:
-#if CONFIG_IS_ENABLED(TARGET_S32G274AEVB) || \
+#if CONFIG_IS_ENABLED(TARGET_S32G2XXAEVB) || \
 		CONFIG_IS_ENABLED(TARGET_S32G274ARDB) || \
 		CONFIG_IS_ENABLED(TARGET_S32G3XXAEVB)
 #if CONFIG_IS_ENABLED(FSL_PFENG_EMAC_1_RGMII)
@@ -502,7 +502,7 @@ static void setup_iomux_pfe(struct udevice *dev,
 		break;
 
 	case PHY_INTERFACE_MODE_RGMII:
-#if CONFIG_IS_ENABLED(TARGET_S32G274AEVB) || \
+#if CONFIG_IS_ENABLED(TARGET_S32G2XXAEVB) || \
 		CONFIG_IS_ENABLED(TARGET_S32G274ARDB) || \
 		CONFIG_IS_ENABLED(TARGET_S32G3XXAEVB)
 #if !CONFIG_IS_ENABLED(FSL_PFENG_EMAC_0_RGMII)

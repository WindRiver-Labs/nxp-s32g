From 96433b34eca7724825d7e2cf8ed1b34de7769702 Mon Sep 17 00:00:00 2001
From: Radu Pirea <radu-nicolae.pirea@nxp.com>
Date: Mon, 1 Feb 2021 10:21:02 +0200
Subject: [PATCH 66/76] dts: s32r45evb: fix gpio node compatible string

Issue: ALB-6406

Signed-off-by: Radu Pirea <radu-nicolae.pirea@nxp.com>
---
 arch/arm/dts/fsl-s32r45.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/dts/fsl-s32r45.dts b/arch/arm/dts/fsl-s32r45.dts
index c04094f5ac..0658f318c6 100644
--- a/arch/arm/dts/fsl-s32r45.dts
+++ b/arch/arm/dts/fsl-s32r45.dts
@@ -74,7 +74,7 @@
 		};
 
 		gpio0: siul2-gpio0 {
-			compatible = "fsl,s32gen1-siul2-gpio";
+			compatible = "fsl,s32-gen1-siul2-gpio";
 			#gpio-cells = <2>;
 			gpio-controller;
 			gpio-ranges = <&pinctrl0 0 0 102>;
@@ -98,7 +98,7 @@
 		};
 
 		gpio1: siul2-gpio1 {
-			compatible = "fsl,s32gen1-siul2-gpio";
+			compatible = "fsl,s32-gen1-siul2-gpio";
 			#gpio-cells = <2>;
 			gpio-controller;
 			gpio-ranges = <&pinctrl1 0 102 32>;
-- 

From 883bb5f8536367da3162668813baf182a75e81f6 Mon Sep 17 00:00:00 2001
From: Ciprian Costea <ciprianmarian.costea@nxp.com>
Date: Mon, 7 Feb 2022 17:19:56 +0200
Subject: [PATCH 59/64] scmi: Enable clocks over SCMI by default

Issue: ALB-8356
Upstream-Status: Pending 

Signed-off-by: Ciprian Costea <ciprianmarian.costea@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 arch/arm/dts/fsl-s32-gen1.dtsi | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/dts/fsl-s32-gen1.dtsi b/arch/arm/dts/fsl-s32-gen1.dtsi
index 29681580a4..b78e0bd645 100644
--- a/arch/arm/dts/fsl-s32-gen1.dtsi
+++ b/arch/arm/dts/fsl-s32-gen1.dtsi
@@ -202,9 +202,9 @@
 			#address-cells = <1>;
 			#size-cells = <0>;
 
-			status = "disabled";
+			status = "okay";
 
-			protocol@14 {
+			clks: protocol@14 {
 				#address-cells = <2>;
 				#size-cells = <2>;
 
@@ -225,12 +225,12 @@
 		status = "disabled";
 	};
 
-	clks: clks {
+	clks {
 		compatible = "fsl,s32-gen1-clocking";
 		#address-cells = <2>;
 		#size-cells = <2>;
 		#clock-cells = <1>;
-		status = "okay";
+		status = "disabled";
 
 		assigned-clocks =
 			<&clks S32GEN1_SCMI_CLK_SERDES_REF>,
-- 
2.17.1


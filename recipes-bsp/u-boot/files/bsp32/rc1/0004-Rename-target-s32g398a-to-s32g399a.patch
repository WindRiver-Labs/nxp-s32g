From ef8eaf28b41889c89fec08724311a45490206228 Mon Sep 17 00:00:00 2001
From: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Date: Mon, 15 Nov 2021 14:39:51 +0200
Subject: [PATCH 04/20] Rename target s32g398a to s32g399a

Issue: ALB-8146
Upstream-Status: Pending 

Signed-off-by: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 arch/arm/cpu/armv8/s32/Kconfig                             | 4 ++--
 arch/arm/cpu/armv8/s32/s32-gen1/soc.c                      | 2 +-
 arch/arm/dts/Makefile                                      | 2 +-
 arch/arm/dts/{fsl-s32g398aemu.dts => fsl-s32g399aemu.dts}  | 0
 board/freescale/s32-gen1/Kconfig                           | 4 ++--
 board/freescale/s32-gen1/Makefile                          | 2 +-
 board/freescale/s32-gen1/board_common.h                    | 2 +-
 board/freescale/s32-gen1/common.c                          | 6 +++---
 configs/{s32g398a_emu_defconfig => s32g399a_emu_defconfig} | 4 ++--
 drivers/net/dwc_eth_qos_s32cc.c                            | 2 +-
 include/configs/{s32g398a.h => s32g399a.h}                 | 6 +++---
 11 files changed, 17 insertions(+), 17 deletions(-)
 rename arch/arm/dts/{fsl-s32g398aemu.dts => fsl-s32g399aemu.dts} (100%)
 rename configs/{s32g398a_emu_defconfig => s32g399a_emu_defconfig} (89%)
 rename include/configs/{s32g398a.h => s32g399a.h} (76%)

diff --git a/arch/arm/cpu/armv8/s32/Kconfig b/arch/arm/cpu/armv8/s32/Kconfig
index 0512935f55..a972a66844 100644
--- a/arch/arm/cpu/armv8/s32/Kconfig
+++ b/arch/arm/cpu/armv8/s32/Kconfig
@@ -111,8 +111,8 @@ config TARGET_S32G274AEMU
 	bool "Support s32g274a ZeBu"
 	select NXP_S32G2XX
 
-config TARGET_S32G398AEMU
-	bool "Support S32G398A ZeBu"
+config TARGET_S32G399AEMU
+	bool "Support S32G399A ZeBu"
 	select NXP_S32G3XX
 
 config TARGET_S32G3XXAEVB
diff --git a/arch/arm/cpu/armv8/s32/s32-gen1/soc.c b/arch/arm/cpu/armv8/s32/s32-gen1/soc.c
index a2d5d0e1c9..ea1b0295a2 100644
--- a/arch/arm/cpu/armv8/s32/s32-gen1/soc.c
+++ b/arch/arm/cpu/armv8/s32/s32-gen1/soc.c
@@ -181,7 +181,7 @@ int print_cpuinfo(void)
 	       get_siul2_midr1_minor(),
 		   !get_siul2_midr2_subminor());
 #elif defined(CONFIG_NXP_S32G3XX)
-	printf("CPU:\tNXP S32G398A\n");
+	printf("CPU:\tNXP S32G399A\n");
 #endif
 	printf("Reset cause: %s\n", get_reset_cause());
 
diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index 3d3105da70..18a5a3038c 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -392,7 +392,7 @@ dtb-$(CONFIG_TARGET_S32G274ABLUEBOX3) += fsl-s32g274abluebox3.dtb
 dtb-$(CONFIG_TARGET_S32G2XXAEVB) += fsl-s32g274aevb.dtb
 dtb-$(CONFIG_TARGET_S32G3XXAEVB) += fsl-s32g3xxaevb.dtb
 dtb-$(CONFIG_TARGET_S32G274AEMU) += fsl-s32g274aemu.dtb
-dtb-$(CONFIG_TARGET_S32G398AEMU) += fsl-s32g398aemu.dtb
+dtb-$(CONFIG_TARGET_S32G399AEMU) += fsl-s32g399aemu.dtb
 dtb-$(CONFIG_S32G274ARDB2) += fsl-s32g274ardb2.dtb
 dtb-$(CONFIG_TARGET_S32R45EVB) += fsl-s32r45.dtb
 
diff --git a/arch/arm/dts/fsl-s32g398aemu.dts b/arch/arm/dts/fsl-s32g399aemu.dts
similarity index 100%
rename from arch/arm/dts/fsl-s32g398aemu.dts
rename to arch/arm/dts/fsl-s32g399aemu.dts
diff --git a/board/freescale/s32-gen1/Kconfig b/board/freescale/s32-gen1/Kconfig
index 61792d4472..347889f615 100644
--- a/board/freescale/s32-gen1/Kconfig
+++ b/board/freescale/s32-gen1/Kconfig
@@ -1,4 +1,4 @@
-if TARGET_S32G274AEMU || TARGET_S32R45EMU || TARGET_S32G398AEMU
+if TARGET_S32G274AEMU || TARGET_S32R45EMU || TARGET_S32G399AEMU
 
 config TARGET_TYPE_S32GEN1_EMULATOR
 	bool
@@ -76,7 +76,7 @@ config SYS_CONFIG_NAME
 	string
 	default "s32g274a" if NXP_S32G2XX
 	default "s32r45" if NXP_S32R45
-	default "s32g398a" if NXP_S32G3XX
+	default "s32g399a" if NXP_S32G3XX
 
 config SYS_ERRATUM_ERR050543
 	bool "Workaround for NXP Erratum ERR050543"
diff --git a/board/freescale/s32-gen1/Makefile b/board/freescale/s32-gen1/Makefile
index d7b36204db..5ae401ca7d 100644
--- a/board/freescale/s32-gen1/Makefile
+++ b/board/freescale/s32-gen1/Makefile
@@ -22,7 +22,7 @@ obj-$(CONFIG_TARGET_S32G274ARDB) += s32g274ardb.o
 obj-$(CONFIG_TARGET_S32G274ASIM) += s32g274asim.o
 obj-$(CONFIG_TARGET_S32G274ABLUEBOX3) += s32g274abluebox3.o
 obj-$(CONFIG_TARGET_S32G274AEMU) += s32g_emu.o
-obj-$(CONFIG_TARGET_S32G398AEMU) += s32g_emu.o
+obj-$(CONFIG_TARGET_S32G399AEMU) += s32g_emu.o
 
 obj-$(CONFIG_TARGET_S32R45EVB) += s32r45evb.o
 obj-$(CONFIG_TARGET_S32R45SIM) += s32r45sim.o
diff --git a/board/freescale/s32-gen1/board_common.h b/board/freescale/s32-gen1/board_common.h
index 558328eb35..a6ec796ab4 100644
--- a/board/freescale/s32-gen1/board_common.h
+++ b/board/freescale/s32-gen1/board_common.h
@@ -22,7 +22,7 @@ void setup_iomux_uart0_pc09_pc10(void);
 
 #if defined(CONFIG_TARGET_S32G274ASIM) || \
 	defined(CONFIG_TARGET_S32G274AEMU) || \
-	defined(CONFIG_TARGET_S32G398AEMU)
+	defined(CONFIG_TARGET_S32G399AEMU)
 void setup_iomux_uart1_pb09_pb10(void);
 #endif
 
diff --git a/board/freescale/s32-gen1/common.c b/board/freescale/s32-gen1/common.c
index 7835c29587..260f582a64 100644
--- a/board/freescale/s32-gen1/common.c
+++ b/board/freescale/s32-gen1/common.c
@@ -55,8 +55,8 @@ int checkboard(void)
 	puts("Board:\tVDK for NXP S32G274A VP\n");
 #elif defined(CONFIG_TARGET_S32G274AEMU)
 	puts("Board:\tZeBu model for NXP S32G274A\n");
-#elif defined(CONFIG_TARGET_S32G398AEMU)
-	puts("Board:\tZeBu model for NXP S32G398A\n");
+#elif defined(CONFIG_TARGET_S32G399AEMU)
+	puts("Board:\tZeBu model for NXP S32G399A\n");
 #elif defined(CONFIG_TARGET_S32R45EVB)
 	puts("Board:\tNXP S32R45-EVB\n");
 #elif defined(CONFIG_TARGET_S32R45SIM)
@@ -86,7 +86,7 @@ int ft_board_setup(void *blob, bd_t *bd)
 
 #if defined(CONFIG_TARGET_S32G274ASIM) || \
 	defined(CONFIG_TARGET_S32G274AEMU) || \
-	defined(CONFIG_TARGET_S32G398AEMU)
+	defined(CONFIG_TARGET_S32G399AEMU)
 void setup_iomux_uart1_pb09_pb10(void)
 {
 	/* Muxing for linflex1 */
diff --git a/configs/s32g398a_emu_defconfig b/configs/s32g399a_emu_defconfig
similarity index 89%
rename from configs/s32g398a_emu_defconfig
rename to configs/s32g399a_emu_defconfig
index d8494a2476..b9014bb76f 100644
--- a/configs/s32g398a_emu_defconfig
+++ b/configs/s32g399a_emu_defconfig
@@ -3,7 +3,7 @@ CONFIG_ARCH_S32=y
 CONFIG_ENV_SIZE=0x2000
 CONFIG_ENV_OFFSET=0x1e0000
 CONFIG_NR_DRAM_BANKS=3
-CONFIG_TARGET_S32G398AEMU=y
+CONFIG_TARGET_S32G399AEMU=y
 CONFIG_S32_LPDDR4=y
 CONFIG_DISTRO_DEFAULTS=y
 # CONFIG_SYS_MALLOC_F is not set
@@ -18,7 +18,7 @@ CONFIG_BOARD_EARLY_INIT_F=y
 CONFIG_CMD_MEMTEST=y
 CONFIG_CMD_MMC=y
 CONFIG_CMD_SMC=y
-CONFIG_DEFAULT_DEVICE_TREE="fsl-s32g398aemu"
+CONFIG_DEFAULT_DEVICE_TREE="fsl-s32g399aemu"
 CONFIG_ENV_IS_IN_MMC=y
 CONFIG_DM=y
 # CONFIG_DM_DEVICE_REMOVE is not set
diff --git a/drivers/net/dwc_eth_qos_s32cc.c b/drivers/net/dwc_eth_qos_s32cc.c
index 73e02ea0c0..2ae2de8148 100644
--- a/drivers/net/dwc_eth_qos_s32cc.c
+++ b/drivers/net/dwc_eth_qos_s32cc.c
@@ -261,7 +261,7 @@ static int check_sgmii_cfg(int gmac_no)
 	defined(CONFIG_TARGET_S32G274ARDB) || \
 	defined(CONFIG_TARGET_S32G274ASIM) || \
 	defined(CONFIG_TARGET_S32G274ASIM) || \
-	defined(CONFIG_TARGET_S32G398AEMU)
+	defined(CONFIG_TARGET_S32G399AEMU)
 
 	desired_mode1 = SGMII_XPCS_1G_OP;
 	desired_mode2 = SGMII_XPCS_1G_OP;
diff --git a/include/configs/s32g398a.h b/include/configs/s32g399a.h
similarity index 76%
rename from include/configs/s32g398a.h
rename to include/configs/s32g399a.h
index d95658093f..f28c28b55f 100644
--- a/include/configs/s32g398a.h
+++ b/include/configs/s32g399a.h
@@ -4,11 +4,11 @@
  */
 
 /*
- * Configuration settings for all the Freescale S32G398A boards.
+ * Configuration settings for all the Freescale S32G399A boards.
  */
 
-#ifndef __S32G398A_H
-#define __S32G398A_H
+#ifndef __S32G399A_H
+#define __S32G399A_H
 
 #include <configs/s32-gen1.h>
 

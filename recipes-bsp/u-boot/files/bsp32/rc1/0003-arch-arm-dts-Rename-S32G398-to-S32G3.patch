From 95c0dd53195c68266649f0364c46723899e31ca2 Mon Sep 17 00:00:00 2001
From: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Date: Mon, 15 Nov 2021 14:26:54 +0200
Subject: [PATCH 03/20] arch/arm/dts: Rename S32G398 to S32G3

Issue: ALB-8146
Upstream-Status: Pending 

Signed-off-by: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 arch/arm/dts/{fsl-s32g398a.dtsi => fsl-s32g3.dtsi} | 2 +-
 arch/arm/dts/fsl-s32g398aemu.dts                   | 2 +-
 arch/arm/dts/fsl-s32g3xxaevb.dts                   | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)
 rename arch/arm/dts/{fsl-s32g398a.dtsi => fsl-s32g3.dtsi} (98%)

diff --git a/arch/arm/dts/fsl-s32g398a.dtsi b/arch/arm/dts/fsl-s32g3.dtsi
similarity index 98%
rename from arch/arm/dts/fsl-s32g398a.dtsi
rename to arch/arm/dts/fsl-s32g3.dtsi
index a57f809c15..f8dcc67fdd 100644
--- a/arch/arm/dts/fsl-s32g398a.dtsi
+++ b/arch/arm/dts/fsl-s32g3.dtsi
@@ -12,7 +12,7 @@
 /delete-node/ &gic;
 / {
 	model = "NXP S32G3XX";
-	compatible = "fsl,s32g398", "fsl,s32gen1",
+	compatible = "fsl,s32g399", "fsl,s32gen1",
 			"arm,vexpress,v2p-aarch64", "arm,vexpress";
 
 	mem1: memory@80000000 {
diff --git a/arch/arm/dts/fsl-s32g398aemu.dts b/arch/arm/dts/fsl-s32g398aemu.dts
index 8e2e595249..f8bc534cab 100644
--- a/arch/arm/dts/fsl-s32g398aemu.dts
+++ b/arch/arm/dts/fsl-s32g398aemu.dts
@@ -5,7 +5,7 @@
 
 /dts-v1/;
 #include "fsl-s32g.dtsi"
-#include "fsl-s32g398a.dtsi"
+#include "fsl-s32g3.dtsi"
 
 /delete-node/ &mem1;
 /delete-node/ &mem2;
diff --git a/arch/arm/dts/fsl-s32g3xxaevb.dts b/arch/arm/dts/fsl-s32g3xxaevb.dts
index 5f98441235..91d5fc645a 100644
--- a/arch/arm/dts/fsl-s32g3xxaevb.dts
+++ b/arch/arm/dts/fsl-s32g3xxaevb.dts
@@ -5,5 +5,5 @@
 
 /dts-v1/;
 #include "fsl-s32g.dtsi"
-#include "fsl-s32g398a.dtsi"
+#include "fsl-s32g3.dtsi"
 #include "fsl-s32gxxxaevb.dtsi"
-- 

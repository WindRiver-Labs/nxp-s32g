From e2dcfb37b9ee0c597812135dd883996b1a21b7e3 Mon Sep 17 00:00:00 2001
From: Ciprian Costea <ciprianmarian.costea@nxp.com>
Date: Thu, 17 Feb 2022 10:53:44 +0200
Subject: [PATCH 12/51] s32-gen1: nvmem: Update 'siul2-nvmem' documentation

Issue: ALB-8517
Upstream-Status: Pending 

Signed-off-by: Ciprian Costea <ciprianmarian.costea@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 doc/device-tree-bindings/misc/fsl,s32-siul2-nvmem.yaml | 5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/doc/device-tree-bindings/misc/fsl,s32-siul2-nvmem.yaml b/doc/device-tree-bindings/misc/fsl,s32-siul2-nvmem.yaml
index 55c548a889..961cab996b 100644
--- a/doc/device-tree-bindings/misc/fsl,s32-siul2-nvmem.yaml
+++ b/doc/device-tree-bindings/misc/fsl,s32-siul2-nvmem.yaml
@@ -21,7 +21,8 @@ properties:
     description: |-
       Each compatible string provides an interface for
       SIUL2_0 or SIUL2_1 module.
-      const: fsl,s32-gen1-siul2_0-nvram
+      const: fsl,s32g-siul2_0-nvram
+      const: fsl,s32r-siul2_0-nvram
       const: fsl,s32g2-siul2_1-nvram
       const: fsl,s32g3-siul2_1-nvram
       const: fsl,s32r-siul2_1-nvram
@@ -48,7 +49,7 @@ unevaluatedProperties: false
 examples:
   - |
     nvram: nvram@4009C000 {
-      compatible = "fsl,s32gen1-siul2_0-nvmem";
+      compatible = "fsl,s32g-siul2_0-nvmem";
       reg = <0x0 10>;
       #address-cells = <1>;
       #size-cells = <1>;

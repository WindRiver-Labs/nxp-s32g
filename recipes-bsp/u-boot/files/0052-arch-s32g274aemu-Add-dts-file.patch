From f77063fcbfbf8790408e7cf687a407ec5c76256f Mon Sep 17 00:00:00 2001
From: Larisa Grigore <larisa.grigore@nxp.com>
Date: Thu, 19 Nov 2020 13:24:04 +0200
Subject: [PATCH 52/76] arch: s32g274aemu: Add dts file

Issue: ALB-6137
Signed-off-by: Larisa Grigore <larisa.grigore@nxp.com>
---
 arch/arm/dts/Makefile            |  1 +
 arch/arm/dts/fsl-s32g274aemu.dts | 29 +++++++++++++++++++++++++++++
 2 files changed, 30 insertions(+)
 create mode 100644 arch/arm/dts/fsl-s32g274aemu.dts

diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index 9d5db35040..642d985484 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -390,6 +390,7 @@ dtb-$(CONFIG_FSL_LSCH2) += fsl-ls1043a-qds-duart.dtb \
 
 dtb-$(CONFIG_TARGET_S32G274ABLUEBOX3) += fsl-s32g274abluebox3.dtb
 dtb-$(CONFIG_TARGET_S32G274AEVB) += fsl-s32g274aevb.dtb
+dtb-$(CONFIG_TARGET_S32G274AEMU) += fsl-s32g274aemu.dtb
 dtb-$(CONFIG_S32G274ARDB) += fsl-s32g274ardb.dtb
 dtb-$(CONFIG_S32G274ARDB2) += fsl-s32g274ardb2.dtb
 dtb-$(CONFIG_TARGET_S32R45EVB) += fsl-s32r45.dtb
diff --git a/arch/arm/dts/fsl-s32g274aemu.dts b/arch/arm/dts/fsl-s32g274aemu.dts
new file mode 100644
index 0000000000..08e73d2165
--- /dev/null
+++ b/arch/arm/dts/fsl-s32g274aemu.dts
@@ -0,0 +1,29 @@
+// SPDX-License-Identifier: GPL-2.0-or-later
+/*
+ * Copyright 2021 NXP
+ */
+
+/dts-v1/;
+#include "fsl-s32g274a.dtsi"
+
+/ {
+	memory@80000000 {
+		device_type = "memory";
+		reg = <0 0x80000000 0 0x80000000>;
+	};
+
+	memory@880000000 {
+		device_type = "memory";
+		reg = <0x8 0x80000000 0 0x80000000>;
+	};
+
+	sram@34000000 {
+		device_type = "memory";
+		reg = <0 0x34000000 0 0x800000>;
+	};
+
+};
+
+&usdhc0 {
+	status = "okay";
+};
-- 

From 9287d7507107496072f0e35d1bc9096cf69dc12e Mon Sep 17 00:00:00 2001
From: Larisa Grigore <larisa.grigore@nxp.com>
Date: Thu, 1 Jul 2021 11:10:07 +0300
Subject: [PATCH 02/18] s32g: Remove QSPI frequency for S32G274A Rev 1

Upstream-Status: Pending

Issue: ALB-7394
Signed-off-by: Larisa Grigore <larisa.grigore@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 drivers/nxp/s32g/clk/s32gen1_clk.c             | 4 ++--
 include/dt-bindings/clock/s32gen1-clock-freq.h | 2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/drivers/nxp/s32g/clk/s32gen1_clk.c b/drivers/nxp/s32g/clk/s32gen1_clk.c
index f8f76ebef..50d8cbdc1 100644
--- a/drivers/nxp/s32g/clk/s32gen1_clk.c
+++ b/drivers/nxp/s32g/clk/s32gen1_clk.c
@@ -358,11 +358,11 @@ static struct s32gen1_clk cgm0_mux12_clk =
 static struct s32gen1_cgm_div qspi_div =
 		S32GEN1_CGM_DIV_INIT(cgm0_mux12_clk, 0);
 static struct s32gen1_clk qspi_2x_clk =
-		S32GEN1_FREQ_MODULE_CLK(qspi_div, 0, S32GEN1_QSPI_MAX_FREQ * 2);
+		S32GEN1_FREQ_MODULE_CLK(qspi_div, 0, S32GEN1_QSPI_CLK_FREQ * 2);
 static struct s32gen1_fixed_div qspi_div2 =
 		S32GEN1_FIXED_DIV_INIT(qspi_2x_clk, 2);
 static struct s32gen1_clk qspi_clk =
-		S32GEN1_FREQ_MODULE_CLK(qspi_div2, 0, S32GEN1_QSPI_MAX_FREQ);
+		S32GEN1_FREQ_MODULE_CLK(qspi_div2, 0, S32GEN1_QSPI_CLK_FREQ);
 
 /* SDHC_CLK */
 static struct s32gen1_mux cgm0_mux14 =
diff --git a/include/dt-bindings/clock/s32gen1-clock-freq.h b/include/dt-bindings/clock/s32gen1-clock-freq.h
index 2b8ab9d7a..286cf098c 100644
--- a/include/dt-bindings/clock/s32gen1-clock-freq.h
+++ b/include/dt-bindings/clock/s32gen1-clock-freq.h
@@ -22,7 +22,7 @@
 #define S32GEN1_PERIPH_PLL_PHI0_MIN_FREQ	(100 * MHZ)
 #define S32GEN1_PERIPH_PLL_PHI2_MIN_FREQ	(40 * MHZ)
 #define S32GEN1_PERIPH_DFS3_FREQ		(800 * MHZ)
-#define S32GEN1_QSPI_MAX_FREQ			(200 * MHZ)
+#define S32GEN1_QSPI_CLK_FREQ			(200 * MHZ)
 #define S32G274A_REV2_QSPI_FREQ			(200 * MHZ)
 #define S32G274A_QSPI_FREQ			(133333333)
 
-- 
